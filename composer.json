{
    "name": "FerhanGit/MultiplicationTable",
    "description": "Display prime numbers multiplication table in terminal",
    "type": "library",
    "keywords": [
        "array",
        "text",
        "table",
        "terminal",
        "ascii",
        "unicode"
    ],
    "homepage": "https://github.com/FerhanGit/MultiplicationTable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ferhan Ismailov",
            "email": "floorer@gbg.bg",
            "homepage": "https://github.com/FerhanGit"
        }
    ],
    "autoload": {
        "psr-4": {
            "Feri\\Multiplication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feri\\Multiplication\\Tests\\": "src/test/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "zendframework/zend-text": "~2.5",
        "phplucidframe/console-table": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": [
            "php ./vendor/bin/phpunit --configuration tests/phpunit.xml --debug"
        ]
    }
}